<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="roundResults.RoundResultsController">
   <children>
      <BorderPane layoutY="-1.0" prefHeight="600.0" prefWidth="850.0">
         <left>
            <ButtonBar prefHeight="540.0" prefWidth="47.0" BorderPane.alignment="CENTER">
               <buttons>
                  <ImageView fx:id="back_arrow" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#goBackToStatisticsScreen" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/back_arrow.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
               </buttons></ButtonBar>
         </left>
         <center>
            <StackPane BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="label" layoutX="100.0" layoutY="100.0" prefHeight="52.0" prefWidth="159.0" StackPane.alignment="BOTTOM_CENTER">
                     <StackPane.margin>
                        <Insets bottom="100.0" />
                     </StackPane.margin>
                     <font>
                        <Font size="25.0" />
                     </font></Label>
                  <ImageView fx:id="image" fitHeight="529.0" fitWidth="770.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
